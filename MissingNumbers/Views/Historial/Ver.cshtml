@model Model.Historial
@{
    ViewBag.Title = "Ver";
}
<div class="container">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4>Detalle Busqueda</h4>
                <button class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="modal-body">
                <dl class="dl-horizontal">
                    <dt>Fecha Consulta</dt>
                    <dt>@Model.Fecha</dt>
                    <dt>Lista Original</dt>
                    <dt>@Model.ListaOriginal</dt>
                    <dt>Lista Secundaria</dt>
                    <dt>@Model.ListaResultado</dt>
                    <dt>Numeros Faltantes</dt>
                    <dt>@Model.listaPerdidos</dt>
                </dl>
            </div>
            <div class="modal-footer">
                <button class="btn btn-primary" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>
